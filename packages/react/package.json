{
  "name": "@prismiq/react",
  "version": "0.2.4",
  "description": "React SDK for Prismiq embedded analytics",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./charts": {
      "types": "./dist/charts/index.d.ts",
      "import": "./dist/charts/index.js",
      "require": "./dist/charts/index.cjs"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.js",
      "require": "./dist/dashboard/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./export": {
      "types": "./dist/export/index.d.ts",
      "import": "./dist/export/index.js",
      "require": "./dist/export/index.cjs"
    },
    "./ssr": {
      "types": "./dist/ssr/index.d.ts",
      "import": "./dist/ssr/index.js",
      "require": "./dist/ssr/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "size": "tsup && du -sh dist/*",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "peerDependencies": {
    "echarts": "^5.4.0 || ^6.0.0",
    "echarts-for-react": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-grid-layout": "^2.0.0",
    "xlsx": "^0.18.0"
  },
  "peerDependenciesMeta": {
    "echarts": {
      "optional": true
    },
    "echarts-for-react": {
      "optional": true
    },
    "xlsx": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.3.7",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "react": "^18.2.0",
    "react-grid-layout": "^2.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "xlsx": "^0.18.5"
  },
  "keywords": [
    "react",
    "analytics",
    "dashboard",
    "embedded",
    "postgresql",
    "visualization",
    "charts",
    "echarts",
    "query-builder"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/statisfy-us/prismiq.git",
    "directory": "packages/react"
  }
}
